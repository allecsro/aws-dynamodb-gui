{
  "name": "dynamodb-gui",
  "version": "1.0.0",
  "description": "Minimalistic GUI for Amazon DynamoDB",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./app/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "Alecsandru DINU <dinu@alecsandru.ro>",
  "license": "MIT",
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/tests/setup/"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.372.0",
    "cross-env": "5.0.1",
    "es6-promisify": "^5.0.0",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "noty": "^3.1.4",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^3.1.3",
    "react-json-inspector": "^7.1.1",
    "react-redux": "~6.0.0",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "~4.0.1",
    "redux-devtools": "^3.4.2",
    "redux-promise-middleware": "~5.1.1",
    "redux-thunk": "~2.3.0",
    "store": "^2.0.12",
    "tablesaw": "^3.0.9"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "~4.6.0",
    "css-loader": "0.28.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.8.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "7.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "jest": "21.0.2",
    "node-sass": "^4.11.0",
    "open-browser-webpack-plugin": "0.0.5",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "redux-mock-store": "^1.5.3",
    "replace-webpack-plugin": "^0.1.2",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "spectre.css": "^0.4.7",
    "style-loader": "0.18.2",
    "url-loader": "^1.1.2",
    "webpack": "~4.27.1",
    "webpack-cli": "~3.1.2",
    "webpack-dev-server": "~3.1.10"
  }
}
